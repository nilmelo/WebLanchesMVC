<li class="dropdown">
    <a asp-controller="Lunch" asp-action="List"
	class="dropdown-toggle" data-toggle="dropdown">
	Lanches<b class="caret"></b>
	</a>
    <ul class="dropdown-menu">
        @foreach (var category in Model)
        {
            <li>
                <a asp-controller="Lunch" asp-action="List"
                   asp-route-category="@category.CategoryName">@category.CategoryName</a>
            </li>
        }
        <li class="divider"></li>
        <li>
            <a asp-controller="Lunch" asp-action="List" asp-route-category="">
				Todos os Lanches
			</a>
        </li>
    </ul>
</li>
